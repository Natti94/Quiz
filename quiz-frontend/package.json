{
  "name": "quiz-frontend",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:netlify": "npx --yes netlify-cli@latest dev",
    "dev:netlify:fixed": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/dev/kill-local-host-sessions.ps1 -Force && npx --yes netlify-cli@latest dev --debug --functions %CD%\\netlify\\functions --dir . --port 8888 --filter quiz-frontend",
    "dev:netlify:stub": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/dev/start-netlify-dev-stub.ps1 -Port 8888 -FunctionsPath ..\\quiz-frontend\\netlify\\functions -Filter quiz-frontend",
    "test:netlify-dev": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/dev/test-netlify-dev.ps1",
    "test:netlify-dev:with-tests": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/dev/test-netlify-dev.ps1 -CopyTests",
    "test:netlify-dev:stub": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/dev/test-netlify-dev.ps1 -DevStub",
    "diagnose:llm": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/dev/diagnose-llm.ps1",
    "self-audit": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/dev/self-audit.ps1",
    "check:ollama": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/dev/check-ollama.ps1",
    "fix:npm-lock": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/dev/fix-npm-lock.ps1",
    "kill-local-sessions": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/dev/kill-local-host-sessions.ps1",
    "dev:netlify:open": "powershell -NoProfile -Command \"Start-Process http://localhost:8888; npx --yes netlify-cli@latest dev\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "strip:comments": "node ../scripts/formatting/remove-comments.mjs",
    "format:repo": "node ../scripts/formatting/format-repository.mjs",
    "test": "vitest --run",
    "test:watch": "vitest",
  "test:jest": "node --experimental-vm-modules ../node_modules/jest/bin/jest.js --runInBand"
  },
  "dependencies": {
    "@semantic-release/npm": "10.0.6",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "fast-glob": "^3.3.3",
    "mongodb": "^6.20.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.5",
    "resend": "^6.2.2",
    "strip-comments": "^2.0.1",
    "strip-css-comments": "^5.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "semantic-release": "^21.1.2",
    "vite": "^7.1.2",
    "vitest": "^1.3.0",
    "jest": "^29.6.1"
  },
  "overrides": {}
}
